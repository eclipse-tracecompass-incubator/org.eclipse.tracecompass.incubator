<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.linuxtools.tmf.core.analysis">
      <module
            analysis_module="org.eclipse.tracecompass.incubator.internal.kernel.core.fileaccess.FileAccessAnalysis"
            applies_experiment="true"
            automatic="false"
            icon="icons/fileaccess.png"
            id="org.eclipse.tracecompass.incubator.internal.kernel.core.fileacess"
            name="%kernel.fileaccess.name">
         <tracetype
               applies="true"
               class="org.eclipse.tracecompass.analysis.os.linux.core.trace.IKernelTrace">
         </tracetype>
         <tracetype
               applies="false"
               class="org.eclipse.tracecompass.tmf.core.trace.experiment.TmfExperiment">
         </tracetype>
      </module>
   </extension>
   <extension
         point="org.eclipse.tracecompass.tmf.core.dataprovider">
      <dataProviderFactory
            class="org.eclipse.tracecompass.incubator.internal.kernel.core.fileaccess.FileAccessDataProviderFactory"
            id="org.eclipse.tracecompass.incubator.internal.kernel.core.fileacess.dataprovider">
      </dataProviderFactory>
   </extension>

</plugin>
